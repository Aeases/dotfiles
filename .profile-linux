. "$HOME/.cargo/env"


# Fix for nodejs "ERR_OSSL_EVP_UNSUPPORTED
export NODE_OPTIONS=--openssl-legacy-provider

# Added by Toolbox App
export PATH="$PATH:/var/home/aeases/.local/share/JetBrains/Toolbox/scripts"

# Podman stuff
export PODMAN_USERNS=keep-id
export DOCKER_HOST=unix://$XDG_RUNTIME_DIR/podman/podman.sock

# Deno binaries
export PATH="/var/home/aeases/.deno/bin:$PATH"

# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/var/home/aeases/miniconda3/bin/conda' 'shell.zsh' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/var/home/aeases/miniconda3/etc/profile.d/conda.sh" ]; then
        . "/var/home/aeases/miniconda3/etc/profile.d/conda.sh"
    else
        export PATH="/var/home/aeases/miniconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<
#
# User specific environment
if ! [[ "$PATH" =~ "$HOME/.local/bin:$HOME/bin:" ]]
then
    PATH="$HOME/.local/bin:$HOME/bin:$PATH"
fi
export PATH

eval "$(/home/linuxbrew/.linuxbrew/bin/brew shellenv)"
